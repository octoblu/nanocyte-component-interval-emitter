{
  "name": "nanocyte-component-interval-emitter",
  "version": "1.0.0",
  "description": "Nanocyte interval emitter",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "coverage": "nyc npm test",
    "test:watch": "mocha -w -R mocha-multi --reporter-options spec=-,mocha-osx-reporter=-",
    "mocha:json": "env NPM_ENV=test mocha --reporter json > coverage/mocha.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octoblu/nanocyte-component-interval-emitter.git"
  },
  "keywords": [
    "nanocyte",
    "octoblu",
    "meshblu"
  ],
  "author": "Octoblu, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/octoblu/nanocyte-component-interval-emitter/issues"
  },
  "homepage": "https://github.com/octoblu/nanocyte-component-interval-emitter#readme",
  "devDependencies": {
    "chai": "^3.3.0",
    "coffee-script": "^1.10.0",
    "mocha": "^2.3.3",
    "mocha-multi": "^0.9.1",
    "mocha-osx-reporter": "^0.1.2",
    "nyc": "^8.3.0",
    "sinon": "^1.17.0",
    "sinon-chai": "^2.8.0"
  },
  "nyc": {
    "cache": true,
    "reporter": [
      "text-summary",
      "lcov",
      "json"
    ],
    "extension": [
      ".coffee"
    ]
  }
}
